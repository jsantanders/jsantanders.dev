{
  "name": "jsantanders.dev",
  "version": "3.0.1",
  "description": "Personal blog about software engineering and math",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jsantanders/jsantanders.dev.git"
  },
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "prebuild": "yarn sitemap & yarn rss",
    "prestart": "yarn build",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --cache",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --cache --fix",
    "build:types": "tsc --noEmit --pretty",
    "audit": "audit --omit=dev",
    "prettify": "prettier --write '**/*.{js,jsx,ts,tsx,json}'",
    "typecheck": "yarn typecheck:rest & yarn typecheck:cypress",
    "typecheck:rest": "tsc --noEmit -p .",
    "typecheck:cypress": "tsc --noEmit -p cypress",
    "sitemap": "ts-node scripts/generate-sitemap.ts",
    "rss": "ts-node scripts/generate-rss.ts",
    "cy:install": "cypress install",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e": "start-server-and-test dev 3000 cy:open",
    "test:e2e:run": "start-server-and-test start 3000 cy:run",
    "validate": "lint & typecheck",
    "prepare": "husky install",
    "publish": "ts-node scripts/publish-blog.ts",
    "frontmatter": "ts-node scripts/check-frontmatter.ts"
  },
  "keywords": [
    "dotnet",
    "node",
    "m",
    "react",
    "rust",
    "azure"
  ],
  "author": "Jesus Santander <jsantanders@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jsantanders/jsantanders.dev/issues"
  },
  "homepage": "https://github.com/jsantanders/jsantanders.dev#readme",
  "browserslist": "defaults, not ie <= 11",
